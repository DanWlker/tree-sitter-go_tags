{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "go_tags",
      "camelcase": "goTags",
      "title": "Go Tags",
      "scope": "source.go_tags",
      "file-types": [
        "go_tags"
      ],
      "injection-regex": "^go_tags$",
      "class-name": "TreeSitterGoTags"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "This is a treesitter highlight syntax for go tags",
    "authors": [
      {
        "name": "DanWlker",
        "email": "danielhee2@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/DanWlker/tree-sitter-go_tags"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}