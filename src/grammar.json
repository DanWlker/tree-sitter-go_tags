{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "go_tags",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_tag_definition"
      }
    },
    "_tag_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "statement"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": " "
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "statement_content"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[^:\\s\"]+"
    },
    "statement_content": {
      "type": "PATTERN",
      "value": "[^\"\\\\]*(?:\\\\.[^\"\\\\]*)*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}