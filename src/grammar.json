{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "go_tags",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_tag_definition"
      }
    },
    "_tag_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tag_identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": " "
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "expression_content"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "tag_identifier": {
      "type": "PATTERN",
      "value": "[^:\\s\"]+"
    },
    "expression_content": {
      "type": "PATTERN",
      "value": "[^\"\\\\]*(?:\\\\.[^\"\\\\]*)*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
